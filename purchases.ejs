<%- include('layout', { body: `
  <h2>Data Pembelian</h2>
  <table>
    <tr>
      <th>ID</th>
      <th>Produk</th>
      <th>Qty</th>
      <th>Status</th>
      <th>Aksi</th>
    </tr>
    ${rows.map(r => `
      <tr>
        <td>${r.id}</td>
        <td>${r.produk}</td>
        <td>${r.qty}</td>
        <td>${r.status}</td>
        <td>
          ${r.status === 'active' ? `<a href='/cancel/${r.id}'>Cancel</a>` : '-'}
        </td>
      </tr>`).join('')}
  </table>
` }) %>
